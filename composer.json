{
    "name": "cedtanghe/elixir-http",
    "description": "Component http",
    "version": "1.0.0",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "http"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-http",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-stdlib"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-validator"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-filter"
        }
    ],
    "require": {
        "php": ">=5.4",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\HTTP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\HTTP\\": "tests/"
        }
    },
    "suggest": {
    }
}